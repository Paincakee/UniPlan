<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, viewport-fit=cover">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="/style/main.css" rel="stylesheet" type="text/css">
    <title>Project</title>
</head>

<body id="projecthome">

    <header id="headerproject">Projects
    </header>


    <div id="projectview">
        <% resultProject.data.forEach((row)=> { %>
            <div id="<%= row.id %>" class="item" onclick="handleClick('<%= row.id %>')">
                <div id="pannelcreator">Creator: </div>
                <div id="panneltitle"><%= row.title %></div>
                <div id="pannelcontact">Contact Info: <%= row.contactInfo %></div>
                <div id="pannelprogress">Status: in development</div>
                <div id="panneljoin"><button type="button" class="btn btn-success">Apply</button></div>
                
            </div>
            <% }) %>
    </div>
    <div class="orbs">
        <div id="orb1"></div>
        <div id="orb2"></div>
        <div id="orb3"></div>
    </div>
    <div class="orbs2">
        <div id="orb4"></div>
        <div id="orb5"></div>
        <div id="orb6"></div>
    </div>
</body>

</html>

<script>
    function handleClick(id) {
        //handle click
        console.log(id);
        link = window.location.href
        window.location.href = link + "/" + id;
    }
</script>